{
  "name": "d3luxee-cronapt",
  "version": "1.0.1",
  "author": "Georg Doser",
  "summary": "Configures the Debian package 'cron-apt' to automatically control package updates via apt.",
  "license": "GPL-3.0+",
  "source": "https://github.com/D3luxee/puppet-cronapt",
  "project_page": "https://github.com/D3luxee/puppet-cronapt",
  "issues_url": "https://github.com/D3luxee/puppet-cronapt/issues",
  "tags": ["packages", "apt", "cronapt", "cron-apt"],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "6", "7", "8" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "12.04", "14.04", "16.04" ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement":">= 3.x < 5.0.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.x < 5.0.0"
    }
  ]
}
